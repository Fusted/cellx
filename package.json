{
  "name": "cellx",
  "version": "1.9.0",
  "description": "",
  "main": "dist/cellx.js",
  "typings": "dist/cellx.d.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "rm -rf dist && rollup -c && tsc",
    "test": "rm -rf coverage && jest",
    "coveralls": "cat coverage/lcov.info | node_modules/.bin/coveralls",
    "debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Riim/cellx.git"
  },
  "author": "Dmitry Vibe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Riim/cellx/issues"
  },
  "homepage": "https://github.com/Riim/cellx",
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "awesome-typescript-loader": "^5.2.1",
    "jest": "^24.8.0",
    "rollup": "^1.16.7",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.21.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@riim/next-tick": "^1.2.2"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  }
}
